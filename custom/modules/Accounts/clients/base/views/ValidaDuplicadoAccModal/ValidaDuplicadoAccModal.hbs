                    <th scope="col">RFC</th>
                </tr>
            </thead>
            <tbody>
                {{#each duplicados}}
                    <tr>
                        <td scope="col">{{nivelMatch}}</td>
                        <td scope="col">{{modulo}}</td>
                        <td scope="col">
                            <a href="#{{moduloLink}}/{{id}}" target="_blank">{{nombre}}</a>
