{{#if meta.short}}
    <a class="module-list-link" href="#{{buildRoute module=module}}" data-route="#{{buildRoute module=module}}"
       track="click:{{module}}" tabindex="-1" xmlns="http://www.w3.org/1999/html"><span class="label label-module label-module-mini label-{{module}}"
        {{#if meta.alias}} title="{{module}} {{else}} title="{{str 'LBL_MODULE_NAME' module}} {{/if}}"
        >{{moduleIconLabel module}}</span>

        {{#notEq module "Expediente"}}
            
           
                {{#notEq module "CotizadorProspectos"}}
                    {{#notEq module "ExpedienteUniclick"}}
                        
                            <!--{{#notEq module "Quantico"}}-->
                    {{#if meta.alias}} {{module}} {{else}} {{str 'LBL_MODULE_NAME' module}} {{/if}}
                            <!--{{/notEq}}-->
                       
                    {{/notEq}}
                {{/notEq}}
            
            
        {{/notEq}}

    
    
    {{#eq module "CotizadorProspectos"}} {{str 'Cotizador IPAD'}} {{/eq}}
    {{#eq module "ExpedienteUniclick"}} {{str 'Expediente Uniclick'}} {{/eq}}
    {{#eq module "Expediente"}} {{str 'Expediente Unifin'}} {{/eq}}
    
    <!--{{#eq module "Quantico"}}{{str 'Quantico'}} -->
    {{/eq}}
    </a>
{{else}}

    {{#notEq module "Expediente"}}
        
            {{#notEq module "CotizadorProspectos"}}
                {{#notEq module "ExpedienteUniclick"}}
                    
                    <!-- {{#notEq module "Quantico"}}-->
        <a class="btn btn-invisible btn-link module-name" href="#{{buildRoute module=module}}" data-route="#{{buildRoute module=module}}" track="click:{{module}}" aria-label="{{str 'LBL_MODULE_NAME' module}}">{{str 'LBL_MODULE_NAME' module}}</a>
    {{/notEq}}{{/notEq}}{{/notEq}}<!--{{/notEq}}-->

    {{#eq module "Quantico"}}
        <a class="btn btn-invisible btn-link module-name">Quantico</a>
        <button type="button" title="{{str 'LBL_LINK_MORE' module}}" data-toggle="dropdown" class="btn btn-invisible dropdown-toggle" track="click:{{name}}-dropdown" aria-label="{{str 'LBL_MODULE_NAME' module}} - {{str 'LBL_LINK_MORE' module}}"><i class="fa fa-caret-down"></i></button>
        <div class="dropdown-menu scroll">
            <ul role="menu">
            <li> <a class="ellipsis_inline"  tabindex="-1" href="#{{buildRoute module=module}}" data-route="#bwc/index.php?entryPoint=ListaTareasQuantico&idActiveDirectory={{activeDirectory}}" track="click:{{module}}"><i class="fa fa-bars"></i>Lista de tareas</a></li>

            <li> <a class="ellipsis_inline"  tabindex="-1" href="#{{buildRoute module=module}}" data-route="#bwc/index.php?entryPoint=operacionesCRM&idActiveDirectory={{activeDirectory}}" track="click:{{module}}"><i class="fa fa-table"></i>Gesti√≥n de firmas</a></li>
            <li> <a class="ellipsis_inline"  tabindex="-1" href="#{{buildRoute module=module}}" data-route="#bwc/index.php?entryPoint=CotizadorQuantico&idActiveDirectory={{activeDirectory}}" track="click:{{module}}"><i class="fa fa-table"></i>Cotizador Quantico</a></li>
            <li> <a class="ellipsis_inline"  tabindex="-1" href="#{{buildRoute module=module}}" data-route="#bwc/index.php?entryPoint=HistorialCotizador&idActiveDirectory={{activeDirectory}}" track="click:{{module}}"><i class="fa fa-table"></i>Historial de cotizaciones</a></li>
            <li> <a class="ellipsis_inline"  tabindex="-1" href="#{{buildRoute module=module}}" data-route="#bwc/index.php?entryPoint=OpMasivasRsQuantico&idActiveDirectory={{activeDirectory}}" track="click:{{module}}"><i class="fa fa-tasks"></i>Operaciones Masivas Rs</a></li>

            </ul>
        </div>
    {{/eq}}


    {{#eq module "Expediente"}}
        <a class="btn btn-invisible btn-link module-name">Expedientes</a>
        <button type="button" title="{{str 'LBL_LINK_MORE' module}}" data-toggle="dropdown" class="btn btn-invisible dropdown-toggle" track="click:{{name}}-dropdown" aria-label="{{str 'LBL_MODULE_NAME' module}} - {{str 'LBL_LINK_MORE' module}}"><i class="fa fa-caret-down"></i></button>
        <div class="dropdown-menu scroll">
            <ul role="menu">

                <li> <a class="ellipsis_inline"  tabindex="-1" href="#{{buildRoute module=module}}" data-route="#bwc/index.php?entryPoint=ExpedienteMod" track="click:{{module}}"><i class="fa fa-clipboard"></i>Expediente Unifin</a></li>


                <li> <a class="ellipsis_inline"  tabindex="-1" href="#{{buildRoute module=module}}" data-route="#bwc/index.php?entryPoint=ExpedienteUniclick" track="click:{{module}}"><i class="fa fa-users"></i>Expediente Uniclick</a></li>
            </ul>
        </div>
    {{/eq}}


    {{#if actions}}
    <button type="button" title="{{str 'LBL_LINK_MORE' module}}" data-toggle="dropdown" class="btn btn-invisible dropdown-toggle" track="click:{{name}}-dropdown" aria-label="{{str 'LBL_MODULE_NAME' module}} - {{str 'LBL_LINK_MORE' module}}"><i class="fa fa-caret-down"></i></button>
    <div class="dropdown-menu scroll">
        <ul role="menu">
            {{#each actions}}
                <li><a class="ellipsis_inline" tabindex="-1"
                       data-navbara-menu-item="{{label}}"
                    {{#if event}}
                       href="javascript:void(0);"
                       data-event="{{event}}"
                       track="click:{{event}}"
                    {{else}}
                       href="{{route}}"
                       data-route="{{route}}"
                       track="click:{{route}}"
                    {{/if}}>{{#if icon}}<i class="sicon {{icon}}"></i>{{/if}}{{str label ../module}}</a>
                </li>
            {{/each}}
            <li class="divider" data-container="recently-viewed"></li>
            <li class="divider" data-container="favorites"></li>
        </ul>
    </div>
    {{/if}}
{{/if}}
