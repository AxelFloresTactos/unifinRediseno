<link rel="stylesheet" href="custom/clients/base/views/cm_prospectos_kanban/styles/styles.css">
<div class="contenedor-grid">
    <div class="contenedor-sinContactar">
        <div class="contenedor-header">
            <div class="header-column">
                <h5>Lead sin contactar</h5>
                <img src="custom/include/images/Contacto_icon.png" alt="">
            </div>
            <div>
                <div class="padding">
                    <p>{{registrosKanban.Lead_Sin_Contactar.Total_Registros}} Registro(s)</p>
                </div>
            </div>
        </div>
    </div>

    <div class="contenedor-contactado color-white">
        <div class="contenedor-header">
            <div class="header-column">
                <h5>Prospecto Contactado</h5>
                <img src="custom/include/images/Contacto_icon.png" alt="">
            </div>
            <div class="padding">
                <div>
                    <p>{{registrosKanban.Prospecto_Contactado.Total_Registros}} Registro(s)</p>
                </div>
            </div>
        </div>
    </div>

    <div class="contenedor-interesado color-white">
        <div class="contenedor-header">
            <div class="header-column">
                    <h5>Prospecto Interesado</h5>
                    <img src="custom/include/images/Contacto_icon.png" alt="">
            </div>
            <div class="header-datos">
                    <div>
                    <p>{{registrosKanban.Prospecto_Interesado.Total_Registros}} Registro(s)</p>
                </div>
                <div style="width:100%;text-align: right;margin-right:10px">
                    <span style="background-color:white">
                        <div class="dropdown-field">
                            <ul data-columna="Prospecto_Interesado">
                                <li class="ordenamiento_categoria" data-columna="Prospecto_Interesado" data-value="MA">Monto Asc</li>
                                <li class="ordenamiento_categoria" data-columna="Prospecto_Interesado" data-value="MD">Monto Desc</li>
                                <li class="ordenamiento_categoria" data-columna="Prospecto_Interesado" data-value="NA">Nombre Asc</li>
                                <li class="ordenamiento_categoria" data-columna="Prospecto_Interesado" data-value="ND">Nombre Desc</li>
                            </ul>
                            <input type="hidden" name="myfield" value="">
                        </div>
                        <span><i class="fa fa-solid fa-sort sortInteresado" modo-ordenamiento="DESC"></i></span>
                    </span>
                </div>
                <div class="cantidad-interesado  contenedor-cantidad">
                    <p><span>$</span>{{registrosKanban.Prospecto_Interesado.Total_Monto}}</p>
                </div>
            </div>
        </div>
    </div>

    <div class="contenedor-integracion color-white">
        <div class="contenedor-header">
            <div class="header-column">
                <h5>Prospecto Integración de Expediente</h5>
                <img src="custom/include/images/Contacto_icon.png" alt="">
            </div>
            <div class="header-datos">
               <div>
                    <p>{{registrosKanban.Prospecto_Integracion_Expediente.Total_Registros}} Registro(s)</p>
                </div>
                <div style="width:100%;text-align: right;margin-right:10px">
                    <span style="background-color:white"><i class="fa fa-solid fa-sort"></i></span>
                </div>
                <div class="cantidad-integracion contenedor-cantidad">
                    <p><span>$</span>{{registrosKanban.Prospecto_Integracion_Expediente.Total_Monto}}</p>
                </div>
            </div>
        </div>
    </div>

    <div class="contenedor-credito color-white">
        <div class="contenedor-header">
            <div class="header-column">
                <h5>Prospecto en Crédito</h5>
                <img src="custom/include/images/Contacto_icon.png" alt="">
            </div>
            <div class="header-datos">
                <div>
                    <p>{{registrosKanban.Prospecto_Credito.Total_Registros}} Registro(s)</p>
                </div>
                <div class="cantidad-credito contenedor-cantidad">
                    <div style="width:100%;text-align: right;margin-right:10px">
                        <span style="background-color:white"><i class="fa fa-solid fa-sort"></i></span>
                    </div>
                    <p><span>$</span>{{registrosKanban.Prospecto_Credito.Total_Monto}}</p>
                </div>
            </div>
        </div>
    </div>

    <div class="sinContactar-items border-right">
        {{#each registrosKanban.Lead_Sin_Contactar.Registros}}
            <div class="contenedor-item" data-id="{{Id}}" data-link="{{Modulo}}/{{Id}}" data-nombre="{{Nombre}}" data-tipo="{{Subtipo_Registro}}" data-registro="LSC">
                <div class="contenedor-informacion">
                    <div class="contenedor-nombre">
                        <div class="circulo">
                            <p>UA</p>
                        </div>
                        <div class="nombre">
                            <p data-id="{{Id}}" title="{{Nombre}}">
                                <a href="#{{Modulo}}/{{Id}}" target="_blank">{{Nombre}}</a>
                            </p>
                        </div>
                    </div>
                    <i class="fa fa-star" data-id="{{Id}}" data-modulo="{{Modulo}}" data-columna="Lead_Sin_Contactar"  {{#if Favorito}}style="color: #3399FF"{{/if}}></i>
                    <div class="cuadrado">
                        <p>{{Dias_Etapa}}</p>
                    </div>
                </div>
                <div class="contenedor-detalles">
                    <div>
                        <p>&nbsp;</p>
                    </div>
                    <div>
                        <p><span>$</span>0.00</p>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>

    <div class="contactado-items border-right">
        {{#each registrosKanban.Prospecto_Contactado.Registros}}
            <div class="contenedor-item" data-id="{{Id}}" data-link="{{Modulo}}/{{Id}}" data-nombre="{{Nombre}}" data-tipo="{{Subtipo_Registro}}" data-registro="PC">
                <div class="contenedor-informacion">
                    <div class="contenedor-nombre">
                        <div class="circulo">
                            <p>UA</p>
                        </div>
                        <div class="nombre">
                            <p data-id="{{Id}}" title="{{Nombre}}">
                                <a href="#{{Modulo}}/{{Id}}" target="_blank">{{Nombre}}</a>
                            </p>
                        </div>
                    </div>
                    <i class="fa fa-star" data-id="{{Id}}" data-modulo="{{Modulo}}" data-columna="Prospecto_Contactado" {{#if Favorito}}style="color: #3399FF"{{/if}}></i>
                    <div class="cuadrado">
                        <p>{{Dias_Etapa}}</p>
                    </div>
                </div>
                <div class="contenedor-detalles">
                    <div>
                        <p>&nbsp;</p>
                    </div>
                    <div>
                        <p><span>$</span>0.00</p>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>

     <div class="interesado-items border-right">
        {{#each registrosKanban.Prospecto_Interesado.Registros}}
            <div class="contenedor-item" data-id="{{Id}}" data-link="{{Modulo}}/{{Id}}" data-nombre="{{Nombre}}" data-tipo="{{Subtipo_Registro}}" data-registro="PI">
                <div class="contenedor-informacion">
                    <div class="contenedor-nombre">
                        <div class="circulo">
                            <p>UA</p>
                        </div>
                        <div class="nombre">
                            <p data-id="{{Id}}" title="{{Nombre}}">
                                <a href="#{{Modulo}}/{{Id}}" target="_blank">{{Nombre}}</a>
                            </p>
                        </div>
                    </div>
                    <i class="fa fa-star" data-id="{{Id}}" data-modulo="{{Modulo}}" data-columna="Prospecto_Interesado" {{#if Favorito}}style="color: #3399FF"{{/if}}></i>
                    <div class="cuadrado">
                        <p>{{Dias_Etapa}}</p>
                    </div>
                </div>
                <div class="contenedor-detalles">
                    <div>
                        <p>&nbsp;</p>
                    </div>
                    <div>
                        <p><span>$</span>{{Monto_Cuenta}}</p>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>

     <div class="integracion-items border-right">
        {{#each registrosKanban.Prospecto_Integracion_Expediente.Registros}}
            <div class="contenedor-item" data-tipo="{{Subtipo_Registro}}">
                <div class="contenedor-informacion">
                    <div class="contenedor-nombre">
                        <div class="circulo">
                            <p>UA</p>
                        </div>
                        <div class="nombre">
                            <p data-id="{{Id}}" title="{{Nombre}}">
                                <a href="#{{Modulo}}/{{Id}}" target="_blank">{{Nombre}}</a>
                            </p>
                        </div>
                    </div>
                    <i class="fa fa-star" data-id="{{Id}}" data-modulo="{{Modulo}}" data-columna="Prospecto_Integracion_Expediente" {{#if Favorito}}style="color: #3399FF"{{/if}}></i>
                    <div class="cuadrado">
                        <p>{{Dias_Etapa}}</p>
                    </div>
                </div>
                <div class="contenedor-detalles">
                    <div>
                        <p>&nbsp;</p>
                    </div>
                    <div>
                        <p><span>$</span>{{Monto_Cuenta}}</p>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>

    <div class="credito-items border-right">
        {{#each registrosKanban.Prospecto_Credito.Registros}}
            <div class="contenedor-item" data-tipo="{{Subtipo_Registro}}">
                <div class="contenedor-informacion">
                    <div class="contenedor-nombre">
                        <div class="circulo">
                            <p>UA</p>
                        </div>
                        <div class="nombre">
                            <p data-id="{{Id}}" title="{{Nombre}}">
                                <a href="#{{Modulo}}/{{Id}}" target="_blank">{{Nombre}}</a>
                            </p>
                        </div>
                    </div>
                    <i class="fa fa-star" data-id="{{Id}}" data-modulo="{{Modulo}}" data-columna="Prospecto_Credito" {{#if Favorito}}style="color: #3399FF"{{/if}}></i>
                    <div class="cuadrado">
                        <p>{{Dias_Etapa}}</p>
                    </div>
                </div>
                <div class="contenedor-detalles">
                    <div>
                        <p>&nbsp;</p>
                    </div>
                    <div>
                        <p><span>$</span>{{Monto_Cuenta}}</p>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>

    <div class="checklist-item hide">
        <div class="center-title-checklist">
            
        </div>
    </div>
</div>