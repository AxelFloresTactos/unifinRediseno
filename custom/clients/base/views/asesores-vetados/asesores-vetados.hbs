{{#if this.loadView}}
    <style>
        .container {
            overflow-y:auto;
            display:block;
            width: 100%;
        }

        .cuentasContainer {
            /*margin-top: 2%;*/
            height: 300px;
            overflow-y: auto;
            overflow-x: hidden;
        }

        #containerHeader {
            font-size: 18px;
            margin-top: 2%;
            margin-left: 1%;
            line-height: 10px;
        }

        #successful {
            margin-top: 10%;
            font-size: 25px;
            color: #33800d;
        }
    </style>


    <div class="container" >
        <div class="row-fluid">
            <div style="color: #e2efe2;background-color:#22608c;border-color:#22608c;border-radius: 10px;margin-left:15px;margin-right:15px;height: 35px;display: table;width: 96%;">
                <h1>
                    <p style="vertical-align: middle;  margin: 5px;">
						Asesores Vetados ({{filtros.Total}})
                    </p>
                </h1>
            </div>
        </div>
        <div class="record row-fluid panel_body">
            <div class="span3 record-cell">
                <div class="record-label">Equipo</div>
                <span>
                    <span>
						<select id="Equipos">
							{{#each lista_equipo}}
								<option value="{{@key}}" {{ifSelected @key ../filtros.Equipo}}>{{this}}</option>
							{{/each}}
						</select>
						<p class="help-block"></p>
                    </span>
                </span>
            </div>
            <div class="span3 record-cell">
                <div class="record-label">Puesto</div>
                <span>
                    <span>
						<select id="Puesto" class="span11">
							{{#each lista_puesto}}
								<option value="{{@key}}" {{ifSelected @key ../filtros.Puesto}}>{{this}}</option>
							{{/each}}
						</select>
                        <p class="help-block"></p>
                    </span>
                </span>
            </div>
            <div class="span1 record-cell">
                <div></div>
                <span>
                    <span>
						<h1>Usuario</h1>
                    </span>
                </span>
            </div>
            <div class="span2 record-cell">
                <div class="record-label">Nombre</div>
                <span>
                    <span>
                        <input id="AsesorN" type="text" value="{{filtros.Nombre}}" placeholder="Nombre">
                        <p class="help-block"></p>
                    </span>
                </span>
            </div>
            <div class="span2 record-cell">
                <div class="record-label">Apellido</div>
                <span>
                    <span>
						<input id="AsesorA" type="text" value="{{filtros.Apellidos}}" placeholder="Apellido">
                        <p class="help-block"></p>
                    </span>
                </span>
            </div>
            <div class="span1 record-cell" style="margin-left:0px;">
                <div class="record-label">&nbsp;</div>
                <span>
                    <span>
                        <input type="button" value="Buscar" id="btn_Asesores" class="btn btn-primary" width="20%" />
                    </span>
                </span>
            </div>
        </div>
        <br>
        <div style="display: none" id="successful">
            <p>Los Asesores fueron Vetados</p>
        </div>
        <table class="table table-striped tablavet" align="center">
        <tr>
            <th width="20%" style="text-align:center">VETADO</th>
            <th width="80%" style="text-align:left">ASESOR</th>
        </tr>
        </table>
    <div class="cuentasContainer">
    <table class="table table-striped tablavet" align="center">
        {{#if listausuarios}}
            {{#each listausuarios}}
                <tr class='single'>
                    <td style="text-align:center; width="20%">
                    <span sfuuid='880' class='list'>
                        <input type="checkbox" class="check-vetado"
                               {{#if vetados_chk_c}}checked{{/if}}
                               >
                    </span>
                    </td>
                    <td width="80%">
                    <span sfuuid='880' class='list'>
                          <div class='ellipsis_inline' data-placement='bottom'>
                              <span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{name}}</span>
                          </div>
                    </span>
                    </td>

                </tr>
            {{/each}}

        </table>
        </div>
            <div><p></p></div>
            <table align="left">
                <tr>

                    {{#each meta.panelsTo}}
                        {{#each fields}}
                            <td>
                                {{field ../../this}}
                            </td>
                        {{/each}}
                    {{/each}}
                    <td><input type="button" value="Guardar cambios" id="btn_guardavetados" class="btn btn-success" /></td>
                </tr>
            </table>
        {{else}}
            <div class="row-fluid" style="color:gray;">
                <div class="span12"><center> No se han encontrado registros</center></div>
            </div>
        {{/if}}
        <textarea style="display: none" id="crossSeleccionados" value=""></textarea>
    </div>
{{/if}}
