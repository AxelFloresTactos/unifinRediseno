{{#if this.loadViewAdminBacklog}}
<style>
    .container {
        overflow-y: auto;
        display: block;
        width: 100%;
    }
    #containerHeader {
        font-size: 18px;
        margin-top: 2%;
        margin-left: 1%;
        line-height: 10px;
    }
    .filtros_table {
        margin-left: 1%;
        margin-top: 2%;
    }

    .filtros {
        margin-bottom: 10px !important;
    }

    .table th {
        text-align: left;
        background: #000042 !important;
        color: #f5f5f5;
        font-weight: bold;
        white-space: nowrap;
    }
    .backlogsContainer {
            margin-top: 1%;
            overflow-y: auto;
            height: auto;
    }
    </style>
    <script>
		var $select2 = this.$('.backlogsContainer').find('select.select2');
		$select2.select2();

        var $select2 = this.$('.panel_body').find('select.select2');
		$select2.select2({ width: '100%' });

	</script>
<div class="container">
    <div class="row-fluid">
            <div style="color: #e2efe2;background-color:#22608c;border-color:#22608c;border-radius: 10px;margin-left:15px;margin-right:15px;height: 35px;display: table;width: 96%;">
                <h1>
                    <p style="vertical-align: middle;  margin: 5px;">
						Administración de Backlog
                    </p>
                </h1>
            </div>
    </div>
    <div class="record row-fluid panel_body">
        <div class="span2 record-cell">
            <div class="record-label">Mes</div>
            <span>
                <span>
                    <select id="mes_filtro" class="filtros select2">
                        {{#each meses_list_html}}
                            <option value="{{@key}}" {{ifSelected @key ../mes_filtro}}>{{this}}</option>
                        {{/each}}
                    </select>
					<p class="help-block"></p>
                </span>
            </span>
        </div>

        <div class="span2 record-cell">
            <div class="record-label">Año</div>
            <span>
                <span>
                    <select id="anio_filtro" class="filtros select2">
                        <option value="{{anio_filtro}}">{{getUnifinListValue "anio_list" anio_filtro}}</option>
                        {{#each anio_list_html_filter}}
                            {{#if @key}}
                                <option value="{{@key}}">{{this}}</option>
                            {{/if}}
                        {{/each}}
                    </select>
					<p class="help-block"></p>
                </span>
            </span>
        </div>

        <div class="span2 record-cell">
            <div class="record-label">Estatus</div>
            <span>
                <span>
                    <select id="etapa_filtro" class="filtros select2">
                        {{#each etapa_list_html}}
                            <option value="{{@key}}" {{ifSelected @key ../etapa_filtro}}>{{this}}</option>
                        {{/each}}
                    </select>
					<p class="help-block"></p>
                </span>
            </span>
        </div>

        <div class="span2 record-cell">
            <div class="record-label">Producto</div>
            <span>
                <span>
                    <select id="producto_filtro" class="filtros select2">
                        {{#each producto_list_html}}
                            <option value="{{@key}}" {{ifSelected @key ../producto_filtro}}>{{this}}</option>
                        {{/each}}
                    </select>
					<p class="help-block"></p>
                </span>
            </span>
        </div>

        <div class="span2 record-cell" style="margin-left:0px;">
            <div class="record-label">&nbsp;</div>
            <span>
                <span>
                    <button class="btn btn-primary" id="btn_Buscar_bl" type="button">Buscar
                        <img id='processing_buscar' style="display: none;" width="15" height="5" src="custom/Levementum/images/icon_processing.gif">
                    </button>
                </span>
            </span>
            <span>
                <span>
                    <button class="btn btn-success" id="btn_guardar" type="button">Guardar cambios
                        <img id="processingGuardar" style="display: none;" width="15" height="5" src="custom/Levementum/images/icon_processing.gif">
                    </button>
                </span>
            </span>
        </div>
    </div>
    <div class="record row-fluid panel_body">
        <div class="span2 record-cell">
            <div class="record-label">Actualización Masiva</div>
            <span>
                <span>
                    <select id="campo_actualizacion_masiva" class="filtros select2">
                        <option value="">&nbsp;</option>
                        <option value="probabilidad">Probabilidad</option>
                    </select>
					<p class="help-block"></p>
                </span>
            </span>
        </div>
        <div class="span2 record-cell">
            <div class="record-label">&nbsp;</div>
            <span>
                <span>
                    <input type="text" id="campo_masivo" class="probabilidad_campo_masivo" value="">
                </span>
            </span>
        </div>

        <div class="span2 record-cell">
            <div class="record-label">&nbsp;</div>
            <span>
                <span>
                    <button class="btn btn-success" id="btn_actualizar_valores_masivo" type="button">Actualizar valores
                    </button>
                </span>
            </span>
        </div>

    </div>
    <!-- Termina barra de búsqueda -->
    <div class="row-fluid" style="overflow-x: auto;">
	</div>

    <div class="backlogsContainer row-fluid">
        <table class="table table-striped tablavet">
            <thead>
                <tr>
						<td >
							<label class="record-label" style="font-weight:bold; text-align:center">Equipo</label>
						</td>
						<td >
							<label class="record-label" style="font-weight:bold; text-align:center">Zona</label>
						</td>
						<td >
							<label class="record-label" style="font-weight:bold; text-align:center">Asesor</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Cliente</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Estatus</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Prospecto</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Crédito</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Rechazada</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Sin Solicitud</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Con Solicitud</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Monto</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Comentarios</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Probabilidad</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">BL Estimado</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Tipo</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Tipo OP</label>
						</td>
                        <td width="10%">
							<label class="record-label" style="font-weight:bold; text-align:center">Rango</label>
						</td>
                        <td >
							<label class="record-label" style="font-weight:bold; text-align:center">Actualizado</label>
						</td>
					</tr>
            </thead>
            <tbody>
            {{#each listaBacklogs}}
					<tr data-id="{{id}}" class="registroBL">
						<td class="">	
							<span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{equipo}}</span>
						</td>
						<td class="">
							<span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>ZONA</span>
						</td>
						<td class="">
							<span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{assigned_user_name}}</span>
						</td>
                        <td class="">
							<span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{nombre_cuenta}}</span>
						</td>
                        <td class="">
							<select class="select2 etapa_c" style="width: 100%;" data-field="etapa_c">
								{{#each ../lista_etapas}}
									<option value="{{@key}}" {{ifSelected @key ../etapa_c}}>{{this}}</option>
								{{/each}}
							</select>
						</td>
                        <td class="">
							<div align="middle">
                                <input type="text" class="montos monto_prospecto" style="width:100px" value="{{monto_prospecto_c}}">
                                <p class="help-block"></p>
                            </div>
						</td>
                        <td class="">
							<div align="middle">
                                <input type="text" class="montos monto_credito" style="width:100px" value="{{monto_credito_c}}">
                                <p class="help-block"></p>
                            </div>
						</td>
                        <td class="">
							<div align="middle">
                                <input type="text" class="montos monto_rechazado" style="width:100px" value="{{monto_rechazado_c}}">
                                <p class="help-block"></p>
                            </div>
						</td>
                        <td class="">
							<div align="middle">
                                <input type="text" class="montos monto_sin_solicitud" style="width:100px" value="{{monto_sin_solicitud_c}}">
                                <p class="help-block"></p>
                            </div>
						</td>
                        <td class="">
							<div align="middle">
                                <input type="text" class="montos monto_con_solicitud" style="width:100px" value="{{monto_con_solicitud_c}}">
                                <p class="help-block"></p>
                            </div>
						</td>
                        <td class="">
							<div class="edit disabled"  align="middle">
                                <input type="text" style="width:80%" name="monto" data-id="{{id}}" value="" class="inherit-width montoTotal" disabled="" >
                            </div>
						</td>
                        <td class="">
                            <textarea class="form-control comentarios" name="comentarios" data-id="{{id}}">{{comentarios_c}}</textarea>
						</td>
                        <td class="">
                            <div align="middle">
                                <input type="text" class="probabilidad" style="width:100px" value="{{tct_conversion_c}}">
                                <p class="help-block"></p>
                            </div>
						</td>
                        <td class="">
							<div class="edit disabled"  align="middle">
                                <input type="text" style="width:100px" name="blEstimado" data-id="{{id}}" value="" class="inherit-width blEstimado" disabled="" >
                            </div>
						</td>
                        <td class="">	
							<span class="tipo" style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{tipo_calculado}}</span>
						</td>
                        <td class="">	
							<span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{tipo_op}}</span>
						</td>
                        <td class="">	
                            <div class="edit disabled"  align="middle">
                               <input type="text" name="rangoBL" data-id="" value="{{rango_bl_c}}" class="inherit-width rango_bl" disabled="" > 
                            </div>
						</td>
                        <td class="">
							<div class="edit disabled"  align="middle">
                                <input type="text" style="width:80%" name="actualizado" data-id="{{id}}" value="" class="inherit-width actualizado" disabled="" >
                            </div>
						</td>
					</tr>
				{{/each}}
            </tbody>
        </table>

        <div class="row-fluid">
            
        </div>
    </div>
    <!--
    <div class="row-fluid">
        <table>
            <tr>
                <td class="span12" style="background-color:#f7f8f9">
                    <input type="button" value="Guardar cambios" id="btn_guardar" class="btn btn-success pull-right" style="margin-right:30px"/>
                </td>
				<td></td>
            </tr>
		</table>
    </div>
    -->
</div>
{{/if}}