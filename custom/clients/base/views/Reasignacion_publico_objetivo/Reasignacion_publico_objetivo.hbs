{{#if this.loadView}}
    <style>
        .container {
            overflow-y: auto;
            display: block;
            width: 100%;
        }

        .cuentas {
            display: table;
            width: 90%;
        }

        .cuentasContainer {
            margin-top: 1%;
            overflow-y: auto;
            height: 300px;
        }

        #containerHeader {
            font-size: 18px;
            margin-top: 2%;
            margin-left: 1%;
            line-height: 10px;
        }

        #successful {
            margin-top: 10%;
            font-size: 25px;
            color: #33800d;
        }
    </style>


    <div class="container">
        <h1 id="containerHeader" align="left" style="color:#22608c"><b>Reasignación de Público Objetivo</b></h1>
        <hr margin: 5px>
        <div class="row-fluid">
            <div style="color: #e2efe2;background-color:#22608c;border-color:#22608c;border-radius: 10px;margin-left:15px;margin-right:15px;height: 35px;display: table;width: 96%;">
                <h1>
                    <p style="vertical-align: middle;  margin: 5px;">
                       Reasignación por filtros
                    </p>
                </h1>
            </div>
        </div>
        <div class="record row-fluid panel_body">

            <div class="span4 record-cell">
                <div class="record-label">Asesor Actual</div>
                <span>
                    <span>
                        {{#each meta.panels}}
                            {{#each fields}}
                                {{field ../../this}}
                            {{/each}}
                        {{/each}}
                        <p class="help-block"></p>
                    </span>
                </span>
            </div>

            <div class="span4 record-cell">
                <div class="record-label">Nombre Público Objetivo</div>
                <span>
                    <span>
                        <input id="filtroPO" type="text" value="">
                            <p class="help-block"></p>
                    </span>
                </span>
            </div>

            <div class="span2 record-cell" style="margin-left:0px;">
                <div class="record-label">&nbsp;</div>
                <span>
                    <span>
                        <button class="btn btn-primary" id="btn_PO" type="button">Buscar
                            <img id='processing' style="display: none;" width="15" height="5" src="custom/Levementum/images/icon_processing.gif"></button>
                    </span>
                </span>
            </div>
        </div>

        <div style="display: none" id="successful">
            <p style="text-align: center;">Los registros fueron reasignados</p>
        </div>
        <br>
    <div class="cuentasContainer">
    <table class="cuentas tcenter" align="center" border="1">
        {{#if prospects}}
            <tr>
                <th width="10%"><input type="button" value="Seleccionar Todo" id="btn_STodo" class="btn btn-primary"
                                       btnState="Off"/></th>
                <th width="10%">Nombre</th>
                <th width="60%">Regimen Fiscal</th>
                <th width="10%">Estatus</th>
                <th width="10%">Asesor asignado</th>
            </tr>
            {{#each prospects}}
                <tr class='single'>
                    <td>
                    <span sfuuid='880' class='list'>
                          <div class='ellipsis_inline' data-placement='bottom'>
                            <span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'><input type="checkbox" name="selected" value="{{id}}" class="checksPO"/></span>
                          </div>
                    </span>
                    </td>

                    <td>
                    <span sfuuid='880' class='list'>
                          <div class='ellipsis_inline' data-placement='bottom'>
                              <span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{name_c}}</span>
                          </div>
                    </span>
                    </td>

                    <td>
                    <span sfuuid='880' class='list'>
                        <div class='ellipsis_inline' data-placement='bottom'>
                            <span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{getUnifinListValue 'regimen_fiscal_list' regimen_fiscal_c}}</span>
                        </div>
                    </span>
                    </td>

                    <td>
                    <span sfuuid='880' class='list'>
                        <div class='ellipsis_inline' data-placement='bottom'>
                            <span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{getUnifinListValue 'estatus_po_list' estatus_po_c}}</span>
                        </div>
                    </span>
                    </td>

                    <td>
                    <span sfuuid='880' class='list'>
                        <div class='ellipsis_inline' data-placement='bottom'>
                            <span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{assigned_user_name}}</span>
                        </div>
                    </span>
                    </td>

                </tr>
            {{/each}}
    </table>
    </div>
            <div><p></p></div>
            <table align="left">
                <tr>
                    <td></td>
                    <th width="30%">Reasignar a:</th>
                    {{#each meta.panelsTo}}
                        {{#each fields}}
                            <td colspan="2">
                                {{field ../../this}}
                            </td>
                        {{/each}}
                    {{/each}}
                    <td><input type="button" value="Reasignar" id="btn_ReAsignar" class="btn btn-success"/></td>
                </tr>
            </table>
        {{/if}}
        <textarea style="display: none" id="crossSeleccionados" value=""></textarea>
        <br>
    </div>
{{/if}}