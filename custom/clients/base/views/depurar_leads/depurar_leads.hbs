{{#if this.loadView}}
    <style>
        .container {
            overflow-y: auto;
            display: block;
            width: 100%;
        }

        .leads {
            display: table;
            width: 90%;
        }

        .leadsContainer {
            margin-top: 1%;
            overflow-y: auto;
            height: 300px;
        }

        #containerHeader {
            font-size: 18px;
            margin-top: 2%;
            margin-left: 1%;
            line-height: 10px;
        }

        #successful {
            margin-top: 10%;
            font-size: 25px;
            color: #33800d;
        }
    </style>

    <div class="container">
        <h1 id="containerHeader" align="left" style="color:#22608c"><b>Depuración de Leads</b></h1>
        <hr margin: 5px>
        <div class="row-fluid">
            <div style="color: #e2efe2;background-color:#22608c;border-color:#22608c;border-radius: 10px;margin-left:15px;margin-right:15px;height: 35px;display: table;width: 96%;">
                <h1>
                    <p style="vertical-align: middle;  margin: 5px;">
                       Búsqueda
                    </p>
                </h1>
            </div>
        </div>
        <div class="record row-fluid panel_body">
            <div class="span3 record-cell">
                <div class="record-label">Agente Asignado</div>
                <span>
                    <span>
                        {{#each meta.panels}}
                            {{#each fields}}
                                {{field ../../this}}
                            {{/each}}
                        {{/each}}
                        <p class="help-block"></p>
                    </span>
                </span>
            </div>
            <div class="span4 record-cell">
                <div class="record-label">Nombre Lead</div>
                <span>
                    <span>
                        <input id="filtroLead" type="text" value="">
                                <p class="help-block"></p>
                    </span>
                </span>
            </div>
            <div class="span4 record-cell">
                <div class="record-label">Nombre Carga</div>
                <span>
                    <span>
                        <input id="filtroCarga" type="text" value="">
                                <p class="help-block"></p>
                    </span>
                </span>
            </div>
            <div class="span1 record-cell" style="margin-left:0px;">
                <div class="record-label">&nbsp;</div>
                <span>
                    <span>
                        <button class="btn btn-primary" id="btn_Buscar" type="button">Buscar<img
                                        id='processing' style="display: none;" width="15" height="5"
                                        src="custom/Levementum/images/icon_processing.gif"></button>
                    </span>
                </span>
            </div>
        </div>
        <div style="display: none" id="successful">
            <p style="text-align: center;">Los Leads seleccionados fueron eliminados</p>
        </div>
    <div class="leadsContainer">
	{{#if listausuarios}}
		<table class="leads tcenter" align="center" border="1">
            <tr>
                <th width="10%"><input type="button" value="Seleccionar Todo" id="btn_STodo" class="btn btn-primary"
                                       btnState="Off"/></th>
                <th width="10%">TIPO</th>
                <th width="50%">NOMBRE</th>
                <th width="30%">AGENTE</th>
            </tr>
            {{#each listausuarios}}
                <tr class='single'>
                    <td>
                    <span sfuuid='880' class='list'>
                          <div class='ellipsis_inline' data-placement='bottom'>
                            <span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'><input
                                    type="checkbox" name="selected" value="{{id}}" class="selected"/></span>
                          </div>
                    </span>
                    </td>
                    <td>
                    <span sfuuid='880' class='list'>
                        <div class='ellipsis_inline' data-placement='bottom'>
                            <span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{tipo_registro_c}}</span>
                        </div>
                    </span>
                    </td>
                    <td>
                    <span sfuuid='880' class='list'>
                          <div class='ellipsis_inline' data-placement='bottom'>
                              <span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{name_c}}</span>
                          </div>
                    </span>
                    </td>
                    <td>
                    <span sfuuid='880' class='list'>
                        <div class='ellipsis_inline' data-placement='bottom'>
                            <span style='font-family: Helvetica, Arial, sans-serif; font-size: 12px;'>{{assigned_user_name}}</span>
                        </div>
                    </span>
                    </td>
                </tr>
            {{/each}}
        </table>
    </div>
    <textarea style="display: none" id="crossSeleccionados" value=""></textarea>
	<br>
    <div class="row-fluid">
        <div style="display:table;width:96.5%;display:inline-block;">
		    <div class="span9 record-cell" style="text-align:right">
				<a style="text-align: right;">Total de registros encontrados: {{filtros.Total}}</a>
            </div>
            <div class="span3 record-cell" style="text-align:right">
				<a class="btn btn-danger btnEliminar" style="text-align:right;">Eliminar</a>
		    </div>
        </div>
    </div>
    </div>
	{{/if}}
{{/if}}