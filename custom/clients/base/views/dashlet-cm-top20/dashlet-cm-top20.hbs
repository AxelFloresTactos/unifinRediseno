<style>
#contenedor{
    text-align:center
}

#left{
    float: left;
    width : 40%;
}

#right{
    float: right;
    width : 60%;
}

</style>

<table class="content" >
    <th>
    <td width="45%">
        <table class="table table-bordered table-striped" style=" width:100%;">
            <thead>
                <tr>
                    <td width = "40%"><b>Backlog</b></td>
                    <td width = "20%"><b>Usuario</b></td>
                    <td width = "20%"><b>Etapa</b></td>
                    <td width = "20%"><b>Monto</b></td>
                </tr>
            </thead>
            <tbody>
                {{#each dataResult}}
                <tr>
                    <td class="nombre">{{#if backlog}}<p><a href="{{href}}" target="_blank">{{backlog}}</a></p>{{/if}}</td>
                    <td>{{#if usuario}}{{usuario}}{{/if}}</td>
                    <td>{{#if etapa}}{{etapa}}{{/if}}</td>
                    <td>{{#if text_monto}}{{text_monto}}{{/if}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </td>
    <td >
        <div class="chart-container" style="position: relative; height:30vh; width:50vw">
            <canvas id="top20" ></canvas>
        </div>
    </td>
    </th>
</table>
