<!-- Campo para desplegar Objetivos en minuta:EdiciÃ³n -->
<div>
  <!-- Table list-->
  <div class="list-view">
    <table id="tableResult" class="table table-striped">
        <thead>
            <tr>
                <th width="80%">Objetivo</th>
                <th width="20%" style="text-align:center">Cumplimiento</th>
            </tr>
        </thead>
        <tbody>
          {{#each myData.records}}
            <tr>
                <td class="objetivoSelect">
                  {{name}}
                </td>
                <td class="CheckAction" style="text-align:center" >
                  <button type="button" class="updateRecord" style="border-width:0;background-color: rgba(255,255,255,0.15);">
                    {{#if cumplimiento}}
                      <i class="fa fa-check" style="color: #33800d; font-size:20px"></i>
                    {{else}}
                      <i class="fa fa-check" style="font-size:20px"></i>
                    {{/if}}
                  </button>
                </td>
            </tr>
          {{/each}}
        </tbody>
    </table>
  </div>
</div>
{{#unless hideHelp}}{{#if def.help}}<p class="help-block">{{str def.help module}}</p>{{/if}}{{/unless}}
