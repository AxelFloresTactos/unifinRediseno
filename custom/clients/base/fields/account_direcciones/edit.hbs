<div class="direcciones_list">
    <!-- Existentes -->
<table class="direcciondashlet" style="width: 100%;" cellspacing="1" cellpadding="5" id="{{id}}">
    {{#each direcciones}}
        <tr style="border-bottom: 1px solid;   border-color:#cfcfcf;">
            <td>
                <div>

                    <div class="row-fluid" style="margin-top: 10px;">  <!--Fila 1 -->
                        <div class="span4">

                                <label class="record-label">Tipo</label>
                                <select class="existingTipodedireccion" data-field="tipodedireccion" placeholder="tipo" style="font-size: 15px;font-weight: 500;display:none">
                                    {{#each tipo_direccion_list_hide}}
                                        <option value="{{@key}}" {{ifSelected @key ../tipo_seleccionado_hide}}>{{this}}</option>
                                    {{/each}}
                                </select>
                                <select class="select2 multi_tipo_existing" style="width: 100%" multiple="multiple" size="3" id="states5">
                                    {{#each tipo_direccion_list}}
                                        <option value="{{@key}}">{{this}}</option>
                                        <!-- <option value="{{@key}}" {{ifMSelected @key ../tipos_seleccionados}}>{{this}}</option> -->
                                    {{/each}}
                                </select>
                                <input type="hidden" class="multi_tipo_existing_hidden" data-field="multi_tipo_existing_hidden" value="{{tipos_seleccionados}}">
                        </div>
                         <div class="span4">
                                    <label class="record-label">Tipo de direcci&oacute;n</label>
                                <select class="existingIndicador" data-field="indicador" placeholder="indicador" style="font-size: 15px;font-weight: 500;display:none" id="states5">
                                    {{#each indicador_list_hide}}
                                        <option value="{{@key}}" {{ifSelected @key ../indicador_seleccionado_hide}}>{{this}}</option>
                                    {{/each}}
                                </select>

                                <select class="select2 multi1_n_existing" style="width: 100%" multiple="multiple" size="3">
                                    {{#each indicador_list}}
                                        <option value="{{@key}}">{{this}}</option>
                                    {{/each}}
                                </select>
                                <input type="hidden" class="multi1_n_existing_hidden" data-field="multi1_n_existing_hidden" value="{{indicadores_seleccionados}}">
                          </div>
                    </div>  <!--Cierre Fila1 -->
                    <div class="row-fluid" style="margin-top: 10px;">  <!--- Fila 2-->
                                    <div class="span4">
                                        <label class="record-label">Código Postal</label>
                                        <input type="text" class="postalInputTempExisting" value="{{codigo_postal}}">
                                        <input type="hidden" class="postalHiddenExisting" data-field="postalHiddenExisting" value="{{postal_hidden}}">
                                    </div>
                                    <div class="span4">
                                        <label class="record-label">País</label>
                                        <select class="paisExisting" data-field="paisExisting" placeholder="" style="font-size: 15px;font-weight: 500;">
                                            {{#each lista_paises_existing}}
                                                <!--<option value="{{@key}}">{{this}}</option>-->
                                                <option value="{{@key}}" {{ifSelected @key ../pais_seleccionado}}>{{this}}</option>
                                            {{/each}}
                                        </select>
                                        <i class="fa fa-spinner fa-spin loadingIconPaisExisting" style="display: none"></i>
                                    </div>
                                    <div class="span4">
                                        <label class="record-label">Estado</label>
                                        <select class="estadoExisting" data-field="estadoExisting" placeholder="" style="font-size: 15px;font-weight: 500;">
                                            {{#each lista_estados_existing}}
                                                <!--<option value="{{@key}}">{{this}}</option>-->
                                                <option value="{{@key}}" {{ifSelected @key ../estado_seleccionado}}>{{this}}</option>
                                            {{/each}}
                                        </select>
                                        <i class="fa fa-spinner fa-spin loadingIconEstadoExisting" style="display: none"></i>
                                    </div>
                    </div> <!--Cierre Fila 2 -->
                    <div class="row-fluid" style="margin-top: 10px;"> <!-- Fila 3 -->
                            <div class="span4">
                                <label class="record-label">Municipio</label>
                                <select class="municipioExisting" data-field="municipioExisting" placeholder="" style="font-size: 15px;font-weight: 500;">
                                    {{#each lista_municipios_existing}}
                                        <option value="{{@key}}" {{ifSelected @key ../municipio_seleccionado}}>{{this}}</option>
                                    {{/each}}
                                </select>
                                <i class="fa fa-spinner fa-spin loadingIconMunicipioExisting" style="display: none"></i>
                            </div>
                            <div class="span4">
                                <label class="record-label">Ciudad</label>
                                <select class="ciudadExisting" data-field="ciudadExisting" placeholder="" style="font-size: 15px;font-weight: 500;">
                                    {{#each lista_ciudades_existing}}
                                        <!--<option value="{{@key}}">{{this}}</option>-->
                                        <option value="{{@key}}" {{ifSelected @key ../ciudad_seleccionada}}>{{this}}</option>
                                    {{/each}}
                                </select>
                                <i class="fa fa-spinner fa-spin loadingIconCiudadExisting" style="display: none"></i>
                            </div>
                            <div class="span4">
                                <label class="record-label">Colonia</label>
                                <select class="coloniaExisting" data-field="coloniaExisting" placeholder="" style="font-size: 15px;font-weight: 500;">
                                    {{#each lista_colonias_existing}}
                                        <option value="{{@key}}" {{ifSelected @key ../colonia_seleccionada}}>{{this}}</option>
                                    {{/each}}
                                </select>
                                <i class="fa fa-spinner fa-spin loadingIconColoniaExisting" style="display: none"></i>
                            </div>
                    </div>  <!-- Cierre Fila 3 -->
                    <div class="row-fluid" style="margin-top: 10px;"> <!-- Fila 4 -->
                            <div class="span4">
                                <label class="record-label">Calle</label>
                                <input type="text" class="calleExisting inputExisting" placeholder="Calle" value="{{calle}}" data-field="calle"/>
                            </div>
                            <div class="span4">
                                <label class="record-label">Número Exterior y Número Interior</label>
                                <input type="text" class="numExtExisting inputExisting" placeholder="numExt" value="{{numext}}" data-field="numext" style="width: 49%;margin: 0px;">
                                <input type="text" class="numIntExisting inputExisting" placeholder="numInt" value="{{numint}}" data-field="numint" style="width: 49%;margin: 0px;">

                            </div>
                            <div class="span4">
                                <label class="record-label">&nbsp;</label>
                                    <div class="btn-group" data-toggle="buttons-checkbox">
                                        <button type="button" class="{{#if principal}}active {{/if}}is_primary btn btn-edit" data-direccionproperty="principal" rel="tooltip">
                                            <i class="fa fa-star"></i></button>
                                    </div>
                                    <div class="btn-group" data-toggle="buttons-checkbox">
                                        <button type="button" class="{{#if inactivo}}active {{/if}}is_primary btn btn-edit" data-direccionproperty="inactivo" rel="tooltip" title="inactivo">
                                            <i class="fa fa-power-off"></i></button>
                                    </div>
                            </div>

                    </div>  <!-- Cierre Fila 4-->
                </div>
            </td>
        </tr>
    {{/each}}
</table>


</div>
<!-- Fin direcciones existentes -->

<h4>Nueva Dirección</h4>
<div class="control-group direccion">
    <div class="direcciondashlet" style="border: 2px solid lightblue; width: 100%;" cellspacing="1" cellpadding="5">
        <tr>
            <td>
                <div>
                    <div class="row-fluid" style="margin-top: 10px;">  <!--Fila 1 -->
                       <div class="span4">
                            <label class="record-label">Tipo</label>
                            <select id="states5" class="newTipodedireccion" data-field="tipodedireccion" placeholder="tipo" style="font-size: 15px;font-weight: 500;width: 90%;margin: 0px;display:none">
                                {{#each def.dir_tipo_list_html}}
                                    <option value="{{@key}}">{{this}}</option>
                                {{/each}}
                            </select>
                            <select id="tipo_multiselect_id" class="select2 multi_tipo" style="width: 100%" multiple="multiple" size="3">
                                {{#each tipo_direccion_list}}
                                    <option value="{{@key}}">{{this}}</option>
                                {{/each}}
                            </select>
                       </div>

                        <div class="span4">
                            <label class="record-label">Tipo de direcci&oacute;n</label>
                            <select id="states5" class="newIndicador" data-field="indicador" placeholder="indicador" style="font-size: 15px;font-weight: 500;display:none">
                                {{#each def.indicador_html}}
                                    <option value="{{@key}}">{{this}}</option>
                                {{/each}}
                            </select>

                            <select id="indicador_multiselect_id" class="select2 multi1_n" style="width: 100%" multiple="multiple" size="3">
                                {{#each indicador_list}}
                                    <option value="{{@key}}">{{this}}</option>
                                {{/each}}
                            </select>

                        </div>
                    </div> <!-- Cierra fila 1-->
                    <div class="row-fluid" style="margin-top: 10px;">  <!--- Fila 2-->
                        <div class="span4">
                            <label class="record-label">Código Postal</label>
                            <input type="text" id="newPostalInputTemp" class="newPostalInputTemp">
                            <input type="hidden" id="newPostalHidden" data-field="postalHidden">
                        </div>
                        <div class="span4">
                            <label class="record-label">País</label>
                            <select class="newPais" data-field="pais" placeholder="" style="font-size: 15px;font-weight: 500;">
                            </select>
                            <i class="fa fa-spinner fa-spin loadingIcon" style="display: none"></i>
                        </div>
                        <div class="span4">
                            <label class="record-label">Estado</label>
                            <select class="newEstado" data-field="estado" placeholder="" style="font-size: 15px;font-weight: 500;">
                            </select>
                            <i class="fa fa-spinner fa-spin loadingIconEstado" style="display: none"></i>
                        </div>
                    </div> <!-- Cierra fila 2-->
                    <div class="row-fluid" style="margin-top: 10px;">  <!--- Fila 3-->
                        <div class="span4">
                            <label class="record-label">Municipio</label>
                            <select class="newMunicipio" data-field="municipio" placeholder="" style="font-size: 15px;font-weight: 500;">
                            </select>
                            <i class="fa fa-spinner fa-spin loadingIconMunicipio" style="display: none"></i>
                        </div>
                        <div class="span4">
                            <label class="record-label">Ciudad</label>
                            <select class="newCiudad" data-field="ciudad" placeholder="" style="font-size: 15px;font-weight: 500;">
                            </select>
                            <i class="fa fa-spinner fa-spin loadingIconCiudad" style="display: none"></i>
                        </div>
                        <div class="span4">
                            <label class="record-label">Colonia</label>
                            <select class="newColonia" data-field="colonia" placeholder="" style="font-size: 15px;font-weight: 500;">
                            </select>
                            <i class="fa fa-spinner fa-spin loadingIconColonia" style="display: none"></i>
                        </div>
                    </div> <!-- Cierra fila 3-->
                        <div class="row-fluid" style="margin-top: 10px;">  <!--- Fila 4-->
                            <div class="span4">
                                <label class="record-label">Calle</label>
                                <input type="text" class="newCalle" placeholder="Calle" value="{{calle}}" data-field="calle"/>
                            </div>
                            <div class="span4">
                                <label class="record-label">Número Exterior y Número Interior</label>
                                <br>
                                <input type="text" class="newNumExt" placeholder="numExt" value="{{numext}}" data-field="numext" style="width: 49%;margin: 0px;">
                                <input type="text" class="newNumInt" placeholder="numInt" value="{{numint}}" data-field="numint" style="width: 49%;margin: 0px;">
                            </div>
                            <div class="span4" style="margin-top: 20px;">
                                <label class="record-label">&nbsp;</label>
                                <div class="btn-toolbar prevent-mousedown">
                                    <div class="btn-group">
                                        <a href="javascript:void(0)" class="btn addDireccion" rel="tooltip" title="Add Direccion"><i class="fa fa-check"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div> <!-- Cierra fila 4-->
                </div>
            </td>
        </tr>
    </div>
</div>
