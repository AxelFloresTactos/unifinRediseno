<div style="border: 2px solid #055993;border-radius: 12px;padding:10px;">
    <!-- Table list-->
    <td class="list-view">
        <table id="tableReferencias" class="table table-striped">
            <thead>
            <tr bgcolor="#0679c8" style="color:#fff">
                <th >Referencia</th>
                <th >Correo</th>
                <th >Tel√©fono</th>


            </tr>
            </thead>
            <tbody>
            {{#each detalle}}
                <tr>
                    {{#if moral}}
                        <td class="campo1SelectR">
                            {{razon_social}}: {{nombres}} {{apaterno}} {{amaterno}}
                        </td>
                    {{else}}
                        <td class="campo1SelectR">
                            {{nombres}} {{apaterno}} {{amaterno}}
                        </td>
                    {{/if}}

                    <td class="campo2SelectR">
                        {{correo}}
                    </td>
                    <td class="campo3SelectR">
                        {{telefono}}
                    </td>
                </tr>

            {{/each}}
            </tbody>
        </table>
    </div>
</div>
