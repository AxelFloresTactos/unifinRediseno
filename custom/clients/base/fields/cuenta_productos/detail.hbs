<div class="row-fluid">
    <table id="tableCuentaxProducto" width="100%">
        <tr>
            <div class="row-fluid">
                <div class="campo1L span6">
                    <label class="record-label" style="margin-left:0px">Tipo de Cuenta Leasing/Crédito SOS</label>
                    <label class="normal " type="text" style="margin-left:0px" data-name="leasing" />
                    {{#if productos.tipo_registro_l}}{{getUnifinListValue 'tipo_registro_cuenta_list' productos.tipo_registro_l}}{{/if}}
                    {{#if productos.subtipo_cuenta_l}}{{getUnifinListValue 'subtipo_registro_cuenta_list' productos.subtipo_cuenta_l}}{{/if}}
                </div>

                <div class="campo2f span6">
                    <label class="record-label" style="margin-left:0px">Tipo de Cuenta Factoraje</label>
                    <label class="normal " type="text" style="margin-left:0px" data-name="factoring" />
