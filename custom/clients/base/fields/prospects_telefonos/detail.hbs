<table class="table table-striped table-condensed span12">
  <thead>
    <tr>
      <th>Tipo Teléfono</th>
      <th>Teléfono</th>
      <th>Llamar</th>
      <th>Estatus Reus</th>
	  <th>Estatus Teléfono</th>
    </tr>
  </thead>
  <tbody>
    <tr>
        <td>TELEFONO TRABAJO</td>
		<td>{{phone_work}}</td>
        {{#if o_registro_reus_c}}
          <td align="center"><button class="mcall sicon sicon-phone btn-danger" style="color: black; pointer-events:none;" disabled></button></td>
        {{else}}
          <td align="center"><button class="mcall sicon sicon-phone btn-success" style="color: white;"></button></td>
        {{/if}}
        {{#if o_registro_reus_c}}
          <td>Registrado en Reus</td>
		{{else}}
		  <td></td>
		{{/if}}
		{{#if o_estatus1}}
			<td>
			  <button class="fa fa-exclamation btn-warning" style="width: 100%; height: 100%; color: black; pointer-events:none;" disabled> {{o_estatus}}</button>
			  {{#if o_c4}}<br>
				<label class="lbl_c4_phone" style="color: red;">Con reporte de engaño telefónico en las bases de c4</label>
			  {{/if}}
			</td>
		{{/if}}
		{{#if o_estatus2}}
			<td>
			  <button class="fa fa-check btn-success" style="width: 100%; height: 100%; color: black; pointer-events:none;" disabled> {{o_estatus}}</button>
			  {{#if o_c4}}<br>
				<label class="lbl_c4_phone" style="color: red;">Con reporte de engaño telefónico en las bases de c4</label>
			  {{/if}}
			</td>
		{{/if}}
		{{#if o_estatus3}}
			<td>
			  <button class="fa fa-times btn-danger" style="width: 100%; height: 100%; color: black; pointer-events:none;" disabled> {{o_estatus}}</button>
			  {{#if o_c4}}<br>
				<label class="lbl_c4_phone" style="color: red;">Con reporte de engaño telefónico en las bases de c4</label>
			  {{/if}}
			</td>
		{{/if}}
    </tr>
